<!-- Page Heading -->
<h1 class="h3 mb-4 text-gray-800">AdminsBackoffice</h1>

<!-- DataTales Example -->
          <div class="card shadow mb-4">
            <div class="card-header py-3">
              <h6 class="m-0 font-weight-bold text-primary">Listando administradores</h6>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      <th>#</th>
                      <th>Email</th>                      
                      <th>Actions</th>                      
                    </tr>
                  </thead>                  
                  <tbody>                                        
                    <% @admins.each do |admin| %>
                      <tr>
                        <td><%= admin.id %></td>
                        <td><%= admin.email %></td>
                        <td style="width: 150px;">                          
                          <%= link_to '#', class: 'btn btn-info btn-circle' do %>
                            <i class="fas fa-edit"></i>
                          <% end %>
                          <%= link_to '#', class: 'btn btn-danger btn-circle' do %>
                            <i class="fas fa-trash"></i>
                          <% end %>
                        </td>
                      </tr>
                    <% end %>                                        
                  </tbody>
                </table>
              </div>
            </div>
          </div>
